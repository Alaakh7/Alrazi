@using Alrazi.Enums
@using Alrazi.Tools
@model Student
@{
    ViewData["Title"] = "تعديل معلومات الطالب";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-12">
        <div class="page-title-box">
            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="~/Panel">الرئيسية</a></li>
                    <li class="breadcrumb-item active">تعديل معلومات الطالب</li>
                </ol>
            </div>
            <h4 class="page-title">تعديل معلومات الطالب</h4>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-xl-12">
        <div class="card">
            <div class="card-body">

                <h4 class="header-title mb-3"> معلومات الحالة - صعوبات تعلم</h4>
                <div method="post">
                    <input type="hidden" id="EarlyMin" value="@ViewBag.EarlyMin" />
                    <input type="hidden" id="EarlyMax" value="@ViewBag.EarlyMax" />
                    <input type="hidden" id="LDMin" value="@ViewBag.LDMin" />
                    <input type="hidden" id="LDMax" value="@ViewBag.LDMax" />
                    <input type="hidden" id="EQMin" value="@ViewBag.EQMin" />
                    <input type="hidden" id="EQMax" value="@ViewBag.EQMax" />
                    <div class="row">
                        <div class="col-12">
                            <div class="row">
                                <div class="col-4">
                                    <label class="col-md-3 col-form-label" for="FirstName">الاسم الأول</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="FirstName" name="FirstName" value="@Model.FirstName">
                                    </div>
                                </div>
                                <div class="col-4">
                                    <label class="col-md-3 col-form-label" for="LastName">الكنية</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="LastName" name="LastName" value="@Model.LastName">
                                    </div>
                                </div>
                                <div class="col-4">
                                    <label class="col-md-3 col-form-label" for="NationalityId">الجنسية</label>
                                    <div class="col-md-9">
                                        <select required class="form-control" id="NationalityId" name="NationalityId">
                                            @foreach (var item in ViewData["Nationalities"] as List<Nationality>)
                                            {
                                                @if (item.Id == Model.NationalityId)
                                                {
                                                    <option selected value="@item.Id">@item.Name</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.Id">@item.Name</option>
                                                }
                                            }
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="row ">
                                <div class="col-4">
                                    <label class="col-md-3 col-form-label" for="StudyStateDate">تاريخ دراسة الحالة</label>
                                    <div class="col-md-9">
                                        <input type="date" required class="form-control" id="StudyStateDate" name="StudyStateDate" onblur="getAgeDateTime()" value="@DataTypeExtention.GetStringFromDate(Model.StudyStateDate)">
                                    </div>
                                </div>
                                <div class="col-4">
                                    <label class="col-md-3 col-form-label" for="BirthDate">تاريخ الميلاد</label>
                                    <div class="col-md-9">
                                        <input type="date" required class="form-control" id="BirthDate" name="BirthDate" onblur="getAgeDateTime()" value="@DataTypeExtention.GetStringFromDate(Model.BirthDate)">
                                    </div>
                                </div>
                                <div class="col-4">
                                    <label class="col-md-3 col-form-label" for="AgeTimeDate">العمر الزمني</label>
                                    <div class="col-md-9">
                                        <input type="text" disabled class="form-control" id="AgeTimeDate">
                                        <b id="StateInfo" style="color:red"></b>
                                    </div>
                                </div>
                            </div>
                            <div class="row ">
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="BirthPlace">مكان الميلاد</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="BirthPlace" name="BirthPlace" value="@Model.BirthPlace">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="IsMale">الجنس</label>
                                    <div class="col-md-9">
                                        <select class="form-control" id="IsMale" name="IsMale">
                                            @if (Model.IsMale)
                                            {
                                                <option selected value="True">ذكر</option>
                                                <option value="False">أنثى</option>
                                            }
                                            else
                                            {
                                                <option value="True">ذكر</option>
                                                <option selected value="False">أنثى</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="row ">
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="PhoneNumber">رقم الهاتف</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="PhoneNumber" name="PhoneNumber" value="@Model.PhoneNumber">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="Address">العنوان</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="Address" name="Address" value="@Model.Address">
                                    </div>
                                </div>
                            </div>
                            <div class="row ">
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="AccessChannelId">الجهة المحولة</label>
                                    <div class="col-md-9">
                                        <select required class="form-control" id="AccessChannelId" name="AccessChannelId">
                                            @foreach (var item in ViewData["AccessChannels"] as List<AccessChannel>)
                                            {
                                                @if (item.Id == Model.AccessChannelId)
                                                {
                                                    <option selected value="@item.Id">@item.Name</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.Id">@item.Name</option>
                                                }
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="AccessDate">تاريخ التحويل</label>
                                    <div class="col-md-9">
                                        <input type="date" required class="form-control" id="AccessDate" name="AccessDate" value="@DataTypeExtention.GetStringFromDate(Model.AccessDate)">
                                    </div>
                                </div>
                            </div>
                            <div class="row ">
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="DiagnosisNumber">هاتف الجهة المحولة</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="DiagnosisNumber" name="DiagnosisNumber" value="@Model.DiagnosisNumber">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="DiagnosisId">تشخيص المحول</label>
                                    <div class="col-md-9">
                                        <select required class="form-control" required id="DiagnosisId" name="DiagnosisId">
                                            @foreach (var item in ViewData["Diagnoses"] as List<Diagnosis>)
                                            {
                                                @if (item.Id == Model.DiagnosisId)
                                                {
                                                    <option selected value="@item.Id">@item.Name</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.Id">@item.Name</option>
                                                }
                                            }
                                        </select>
                                    </div>
                                </div>
                            </div>



                            <div class="row d-none" id="ldRow">
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="SchoolName">اسم المدرسة</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="SchoolName" name="SchoolName" value="@Model.SchoolName">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class="col-md-3 col-form-label" for="Class">الصف</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="Class" name="Class" value="@Model.Class">
                                    </div>
                                </div>
                            </div>


                            <div class="row">
                                <div class="col-11">
                                    <label class="col-md-6 col-form-label" for="FamilyBio">ملخص المشكلة الحالة (نوعها وأعراضها) كما يرويها ولي الأمر</label>
                                    <textarea class="form-control" required id="FamilyBio" name="FamilyBio">@Model.FamilyBio</textarea>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-11">
                                    <label class="col-md-6 col-form-label" for="MedicalExamination">نتيجة الفحص الطبي</label>
                                    <textarea class="form-control" required id="MedicalExamination" name="MedicalExamination">@Model.MedicalExamination</textarea>
                                </div>
                            </div>

                            <div style="padding-top:20px">
                                <button type="submit" id="btnNext" class="btn btn-primary">حفظ</button>
                            </div>
                        </div> <!-- end col -->
                    </div> <!-- end row -->
                </div>
            </div> <!-- end card-body -->
        </div> <!-- end card-->
    </div> <!-- end col -->
</div>

<div class="row">
    <div class="col-xl-12">
        <div class="card">
            <div class="card-body">
                <h4 class="header-title mb-3"> معلومات عامة عن العائلة</h4>
                <div method="post">
                    <div class="row">
                        <div class="col-12">
                            <div class="row">
                                <div class="col-6">
                                    <label class=" col-form-label" for="FatherName">اسم الأب</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="FatherName" name="FatherName" value="@Model.StudentFamilyInfo.FatherName">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="FatherYear">عمر الأب</label>
                                    <div class="col-md-9">
                                        <input type="number" class="form-control" id="FatherYear" name="FatherYear" value="@Model.StudentFamilyInfo.GetFatherYear">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="FatherStudy">المستوى التعليمي الأب</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="FatherStudy" name="FatherStudy" value="@Model.StudentFamilyInfo.FatherStudy">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="FatherJob">عمل الأب ومكانه</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="FatherJob" name="FatherJob" value="@Model.StudentFamilyInfo.FatherJob">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="MotherName">اسم الأم</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="MotherName" name="MotherName" value="@Model.StudentFamilyInfo.MotherName">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="MotherYear">عمر الأم</label>
                                    <div class="col-md-9">
                                        <input type="number" class="form-control" id="MotherYear" name="MotherYear" value="@Model.StudentFamilyInfo.GetMotherYear">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="MotherStudy">المستوى التعليمي للأم</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="MotherStudy" name="MotherStudy" value="@Model.StudentFamilyInfo.MotherStudy">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="MotherJob">عمل الأم ومكانها</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="MotherJob" name="MotherJob" value="@Model.StudentFamilyInfo.MotherJob">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="FatherAndMotherDegree">درجة القرابة بين الأم والأب</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="FatherAndMotherDegree" name="FatherAndMotherDegree" value="@Model.StudentFamilyInfo.FatherAndMotherDegree">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label">هل يعاني أحد الأبوين من الأمراض</label>
                                    <div class="col-md-9">
                                        <select class="form-control">
                                            @if (string.IsNullOrWhiteSpace(Model.StudentFamilyInfo.FatherAndMotherDiseases))
                                            {
                                                <option>نعم</option>
                                                <option selected>لا</option>
                                            }
                                            else
                                            {
                                                <option selected>نعم</option>
                                                <option>لا</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="FatherAndMotherDiseases">في حال كان يعاني من الأمراض أذكرها</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="FatherAndMotherDiseases" name="FatherAndMotherDiseases" value="@Model.StudentFamilyInfo.FatherAndMotherDiseases">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="SeparatedParents">هل الأبوين منفصلين</label>
                                    <div class="col-md-9">

                                        <select class="form-control">
                                            @if (@Model.StudentFamilyInfo.SeparatedParents == true)
                                            {
                                                <option value="1" selected>نعم</option>
                                                <option value="0">لا</option>
                                            }
                                            else
                                            {
                                                <option value="1">نعم</option>
                                                <option value="0" selected>لا</option>
                                            }
                                        </select>
                                    </div>
                                </div>

                                <div class="col-6">
                                    <label class=" col-form-label" for="ChildResidence">مع من يقيم الطفل.</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="ChildResidence" name="ChildResidence" value="@Model.StudentFamilyInfo.ChildResidence">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="userName">هل يوجد بين أقرباء والديه أفراد معاقين</label>
                                    <div class="col-md-9">
                                        <select class="form-control">
                                            @if (string.IsNullOrWhiteSpace(Model.StudentFamilyInfo.DisabilityOfRelative))
                                            {
                                                <option>نعم</option>
                                                <option selected>لا</option>
                                            }
                                            else
                                            {
                                                <option selected>نعم</option>
                                                <option>لا</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="DisabilityOfRelative">ماهي الإعاقة</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="DisabilityOfRelative" name="DisabilityOfRelative" value="@Model.StudentFamilyInfo.DisabilityOfRelative">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="RelationshipWithDisabled">ماهي صلة القرابة</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="RelationshipWithDisabled" name="RelationshipWithDisabled" value="@Model.StudentFamilyInfo.RelationshipWithDisabled">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="BrotherAndSisterCount">ماهو عدد أخوة الطفل</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="BrotherAndSisterCount" name="BrotherAndSisterCount" value="@Model.StudentFamilyInfo.BrotherAndSisterCount">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="ChildOrder">ترتيب الطفل في الأسرة</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="ChildOrder" name="ChildOrder" value="@Model.StudentFamilyInfo.ChildOrder">
                                    </div>
                                </div>
                                <div style="padding-top:20px">
                                    <button type="submit" class="btn btn-primary">حفظ</button>
                                </div>
                            </div>
                        </div> <!-- end col -->
                    </div> <!-- end row -->
                </div>
            </div> <!-- end card-body -->
        </div> <!-- end card-->
    </div> <!-- end col -->
</div>

<div class="row">
    <div class="col-xl-12">
        <div class="card">
            <div class="card-body">
                <h4 class="header-title mb-3"> معلومات الأشقاء الغير متزوجين</h4>
                <div method="post">
                    <div class="row">
                        <div class="col-12">
                            <div class="row">

                                <table class="table table-centered mb-0">
                                    <thead class="table-dark">
                                        <tr>
                                            <th>البند</th>
                                            <th>الاسم</th>
                                            <th>الجنس</th>
                                            <th>العمر</th>
                                            @if (ViewBag.StudentType == StudentType.StudentLearningDifficulties)
                                            {
                                                <th>الحالة الصحية</th>
                                            }
                                            <th>المستوى التعليمي</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tr>
                                        <td>
                                            <input type="number" id="order" class="form-control" />
                                        </td>
                                        <td>
                                            <input type="text" id="name" class="form-control" />
                                        </td>
                                        <td>
                                            <select class="form-control" id="isMale">
                                                <option value="True">ذكر</option>
                                                <option value="False">أنثى</option>
                                            </select>
                                        </td>
                                        <td>
                                            <input type="number" class="form-control" id="age" />
                                        </td>
                                        @if (ViewBag.StudentType == StudentType.StudentLearningDifficulties)
                                        {
                                            <td>
                                                <input type="text" class="form-control" id="healthStatus" />
                                            </td>
                                        }
                                        else
                                        {
                                            <input type="hidden" class="form-control" id="healthStatus" value="" />
                                        }
                                        <td>
                                            <input type="text" class="form-control" id="level" />
                                        </td>
                                        <td>
                                            <a href="#" onclick="AddRecord()">إضافة</a>
                                        </td>
                                    </tr>
                                    <tbody id="tblData">
                                        @foreach (var item in Model.StudentSiblings)
                                        {
                                            <tr>
                                                <td>@item.Order</td>
                                                <td>@item.FullName</td>
                                                @if (item.IsMale)
                                                {
                                                    <td>ذكر</td>
                                                }
                                                else
                                                {
                                                    <td>أنثى</td>
                                                }
                                                <td>
                                                    @(DateTime.Now.Year - item.BirthDate.Year)
                                                </td>
                                                @if (ViewBag.StudentType == StudentType.StudentLearningDifficulties)
                                                {
                                                    <td>
                                                        @item.HealthStatus
                                                    </td>
                                                }
                                                <td>@item.StudyLevel</td>
                                                <td>
                                                    <a href="#" onclick="RemoveRecord('@item.UId')" class="action-icon"> <i class="mdi mdi-delete"></i></a>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>

                                <div style="padding-top:20px">
                                    <button type="submit" class="btn btn-primary">حفظ</button>
                                </div>

                            </div>
                        </div> <!-- end col -->
                    </div> <!-- end row -->
                </div>
            </div> <!-- end card-body -->
        </div> <!-- end card-->
    </div> <!-- end col -->
</div>

<div class="row">
    <div class="col-xl-12">
        <div class="card">
            <div class="card-body">
                <h4 class="header-title mb-3"> التاريخ الطبي للأم</h4>
                <div method="post">
                    <div class="row">
                        <div class="col-12">
                            <div class="row">
                                <div class="col-6">
                                    <label class=" col-form-label" for="ComplicationsDuringPregnancy">@(DataTypeExtention.GetDisplayName<StudentMotherMedical>("ComplicationsDuringPregnancy"))</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="ComplicationsDuringPregnancy" name="ComplicationsDuringPregnancy" value="@Model.StudentMotherMedical.ComplicationsDuringPregnancy">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="MonthMotherExposedRadiation">@(DataTypeExtention.GetDisplayName<StudentMotherMedical>("MonthMotherExposedRadiation"))</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="MonthMotherExposedRadiation" name="MonthMotherExposedRadiation" value="@Model.StudentMotherMedical.MonthMotherExposedRadiation">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="MotherAtBirthYear">@(DataTypeExtention.GetDisplayName<StudentMotherMedical>("MotherAtBirthYear"))</label>
                                    <div class="col-md-9">
                                        <input type="number" class="form-control" id="MotherAtBirthYear" name="MotherAtBirthYear" value="@Model.StudentMotherMedical.GetMotherAtBirthYear">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="TookMedicationWhilePregnant">@(DataTypeExtention.GetDisplayName<StudentMotherMedical>("TookMedicationWhilePregnant"))</label>
                                    <div class="col-md-9">
                                        <select class="form-control" name="TookMedicationWhilePregnant">
                                            @if (Model.StudentMotherMedical.TookMedicationWhilePregnant)
                                            {
                                                <option selected value="True">نعم</option>
                                                <option value="False">لا</option>
                                            }
                                            else
                                            {
                                                <option value="True">نعم</option>
                                                <option selected value="False">لا</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="IsBlue">@(DataTypeExtention.GetDisplayName<StudentMotherMedical>("IsBlue"))</label>
                                    <div class="col-md-9">
                                        <select class="form-control" name="IsBlue">
                                            @if (Model.StudentMotherMedical.IsBlue)
                                            {
                                                <option selected value="True">نعم</option>
                                                <option value="False">لا</option>
                                            }
                                            else
                                            {
                                                <option value="True">نعم</option>
                                                <option selected value="False">لا</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="PsychologicalState">@(DataTypeExtention.GetDisplayName<StudentMotherMedical>("PsychologicalState"))</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="PsychologicalState" name="PsychologicalState" value="@Model.StudentMotherMedical.PsychologicalState">
                                    </div>
                                </div>
                                <div class="col-6">
                                    <label class=" col-form-label" for="PregnancyDuration">@(DataTypeExtention.GetDisplayName<StudentMotherMedical>("PregnancyDuration"))</label>
                                    <div class="col-md-9">
                                        <input type="text" required class="form-control" id="PregnancyDuration" name="PregnancyDuration" value="@Model.StudentMotherMedical.PregnancyDuration">
                                    </div>
                                </div>
                                <div class="col-6">
